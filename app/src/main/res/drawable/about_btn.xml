<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 按钮默认时显示的图片-->
    <item android:state_pressed="false" android:state_focused="false"
        android:drawable="@drawable/about_1"/>
    <!-- 按钮按下时时显示的图片-->
    <item android:state_pressed="true" android:drawable="@drawable/about_2"/>
    <!--按钮长按时显示的图片-->
    <item android:state_focused="true" android:drawable="@drawable/about_2"/>
</selector>